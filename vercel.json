{
  "version": 2,
  "name": "queen-riam-pair",
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/pair",
      "dest": "/pair.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
